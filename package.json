  },
  "devDependencies": {
    "@types/p5": "^1.7.6",
    "gh-pages": "^6.1.0",
    "typescript": "^5.5.0",
    "vite": "^5.2.0",
    "vite-plugin-singlefile": "^2.0.1",
    "vite-plugin-solid": "^2.11.8"
  },
  "dependencies": {
    "p5": "^2.0.4",
    "solid-js": "^1.9.9"
  }
}
